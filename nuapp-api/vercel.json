{
  "version": 2,
  "builds": [
    {
      "buildCommand": "npm run build",
      "installCommand": "npm install",
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
